# NextAuth
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=

# Google OAuth (admin login via Google — from Google Cloud Console)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# Google Service Account (domain-wide delegation — paste JSON key as single line)
GOOGLE_SERVICE_ACCOUNT_KEY=
GOOGLE_ADMIN_EMAIL=admin@yourcompany.com
GOOGLE_WORKSPACE_DOMAIN=yourcompany.com

# AshbyHQ
ASHBY_API_KEY=

# Vercel Postgres (auto-populated when you add Postgres integration in Vercel)
POSTGRES_URL=
POSTGRES_URL_NON_POOLING=
